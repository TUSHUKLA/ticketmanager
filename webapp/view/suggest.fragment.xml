<core:FragmentDefinition 
xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialog"
		title="TICKET ID:"
		items="{T>/}"
		search=".onValueHelpSearch"
		confirm=".onValueHelpClose"
		cancel=".onValueHelpClose">
		<StandardListItem
		    id="ticket"
			iconDensityAware="false"
			iconInset="false"
			title="{T>ticketid}"
			 />
	</SelectDialog>
</core:FragmentDefinition>